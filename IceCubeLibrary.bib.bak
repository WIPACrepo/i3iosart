@Article{NoiseHLc_dheereman11,
  Title                    = {{Study of noise HLC hits in DeepCore}},
  Author                   = {Heereman, D. and Hanson, K. and Labare, M. and van Eijndhoven, N.},
  Journal                  = {{IceCube Internal Report 201102009}},
  Year                     = {2011},

  Month                    = {February},

  Abstract                 = {The low SMTn rate (n<8) in DeepCore shows a discrepancy between real data and MC simulation. We calculate the SMT3 and SMT4 rate analytically based on the assumption that accidental hits, generated by Poissonian noise, contribute to the increase in real data. Depending on the pure noise rate assumed per DOM (650Hz and 700HZ in DeepCore) we derive an accidental HLC rate of 1.12Hz to 1.39Hz for the SMT3 case and 2.30Hz to 2.78Hz regarding SMT4. Furthermore, a simulation of pure noise accidental hits was implemented and yields values of 1.60Hz for SMT3 and 0.70Hz for SMT4, respectively. Additionally, these results are compared with calcualtions by other authors.},
