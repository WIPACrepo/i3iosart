@PhdThesis{kowarik2010,
  Title                    = {Supernova Neutrinos in AMANDA and IceCube - Monte Carlo Development and Data Analysis},
  Author                   = {Thomas Kowarik},
  School                   = {Johannes Gutenberg-Universit\"at Mainz},
  Year                     = {2010},

  Abstract                 = {Supernovae geh?ren zu den energiereichsten Ereignissen des Universums und stellen die einzig gesicherte extrasolare Neutrinoquelle dar. Da der Mechanismus der Explosion nicht ausreichend verstanden ist, w?re die Messung eines Ausbruchs von Neutrinos aus einem solchen Ereignis ein wichtiger Test, sowohl f?r die Explosionsmodelle als auch f?r die Elementarteilchenphysik. Das sich am Geographischen S?dpol befindende Neutrinoteleskop IceCube weist Tscherenkow-Photonen im antarktischen Gletscher nach. Obwohl es f?r die Detektion hochenergetischer Neutrinos konzipiert wurde, ist es wegen des niedrigen Sensorrauschens im Eis m?glich, den Neutrinoausbruch einer Supernova in der Milchstra?e ?ber eine kollektive Ratenerh?hung nachzuweisen. Mit einem speziell f?r Wasser-Tscherenkow-Teleskope entwickelten Signal-Monte-Carlo wird untersucht, inwiefern IceCube in der Lage ist, zwischen verschiedenen Supernovamodellen und Oszillationsszenarien zu unterscheiden. In einem zweiten Teil werden die mit dem IceCube-Vorl?ufer AMANDA aufgezeichneten neun Jahre an Informationen analysiert. Nach intensiver Datenbereinigung und Entwicklung einer Untergrundsimulation wird eine obere Grenze auf die erwartete Anzahl an Supernovae in der Milchstra?e gesetzt.},
  File                     = {:/home/david/Dropbox/IceCube_Papers/disskow.pdf:PDF},
  Owner                    = {david},
  Timestamp                = {2011.01.12}
}

@Article{sn_amanda_kowarik09,
  Title                    = {Supernova Search with the AMANDA / IceCube Detectors},
  Author                   = {Kowarik, Thomas and Griesel, Timo and Piegsa, Alexander},
  Year                     = {2009},

  Abstract                 = {Since 1997 the neutrino telescope AMANDA at the geographic South Pole has been monitoring our Galaxy for neutrino bursts from supernovae. Triggers were introduced in 2004 to submit burst candidates to the Supernova Early Warning System SNEWS. From 2007 the burst search was extended to the much larger IceCube telescope, which now supersedes AMANDA. By exploiting the low photomultiplier noise in the antarctic ice (on average 280 Hz for IceCube), neutrino bursts from nearby supernovae can be identi?ed by the induced collective rise in the pulse rates. Although only a counting experiment, IceCube will provide the world?s most precise measurement of the time pro?le of a neutrino burst near the galactic center. The sensitivity to neutrino properties such as the theta_13 mixing angle and the neutrino hierarchy are discussed as well as the possibility to detect the deleptonization burst. Keywords: supernova neutrino IceCube},
  Archiveprefix            = {arXiv},
